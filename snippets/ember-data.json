  },
  "peek-record": {
    "prefix": "peek-record",
    "body": "this.store.peekRecord('${1:model}', params.${1:model}_id);",
    "description": "store.peekRecord(...)"
  },
  "query-all": {
    "prefix": "query-all",
    "body": "this.store.query('${1:model}', { ${2:queryKey}: ${3:queryValue}});",
